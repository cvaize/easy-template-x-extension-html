{
  "name": "easy-template-x-extension-html",
  "version": "0.0.3",
  "description": "An extension for the easy-template-x package that allows you to use html to style content in a document.",
  "scripts": {
    "build": "microbundle",
    "dev": "microbundle watch"
  },
  "type": "module",
  "source": "src/index.js",
  "main": "./dist/index.cjs",
  "exports": {
    "require": "./dist/index.cjs",
    "default": "./dist/index.modern.js"
  },
  "module": "./dist/index.module.js",
  "unpkg": "./dist/index.umd.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cvaize/easy-template-x-extension-html.git"
  },
  "keywords": [
    "easy-template-x",
    "docx",
    "html",
    "template",
    "word",
    "office",
    "microsoft",
    "ms"
  ],
  "bugs": {
    "url": "https://github.com/cvaize/easy-template-x-extension-html/issues"
  },
  "homepage": "https://github.com/cvaize/easy-template-x-extension-html#readme",
  "author": "Dmitry Orlov",
  "license": "ISC",
  "dependencies": {
    "easy-template-x": "^3.0.2"
  },
  "devDependencies": {
    "microbundle": "^0.15.1"
  }
}
